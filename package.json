{
  "name": "frayt-driver-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/FraytDriverV3/main.jsbundle --sourcemap-output ios/FraytDriverV3/main.jsbundle.map --dev=false --verbose",
    "test:unit": "jest --testPathPattern=__tests__/unit --runInBand --detectOpenHandles --forceExit",
    "test:e2e-android": "npx detox test --configuration android.emu.release",
    "test:e2e-android-build": "MY_APP_MODE=mocked npx detox build --configuration android.emu.release",
    "test:e2e-ios": "npx detox test --configuration ios.sim.release",
    "test:e2e-ios-build": "MY_APP_MODE=mocked npx detox build --configuration ios.sim.release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@frayt/react-interval-hook": "^1.1.3",
    "@frayt/sdk": "1.0.82",
    "@fullstory/babel-plugin-annotate-react": "^2.2.2",
    "@fullstory/react-native": "^1.1.1",
    "@intercom/intercom-react-native": "^4.0.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-picker/picker": "^2.4.10",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7",
    "@reduxjs/toolkit": "^1.9.1",
    "@sentry/react-native": "^5.4.2",
    "@stripe/stripe-react-native": "^0.23.3",
    "@ui-kitten/components": "^5.1.2",
    "@ui-kitten/eva-icons": "5.1.2",
    "@ui-kitten/metro-config": "5.1.0",
    "axios": "^1.3.1",
    "expo": "^47.0.0",
    "expo-app-loading": "^2.1.1",
    "expo-gradle-ext-vars": "^0.1.1",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.8",
    "react-native-animated-pagination-dots": "^0.1.73",
    "react-native-background-actions": "^3.0.0",
    "react-native-background-fetch": "^4.1.9",
    "react-native-background-geolocation-android": "github:transistorsoft/react-native-background-geolocation-android",
    "react-native-camera-kit": "^13.0.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-code-push": "^7.1.0",
    "react-native-device-info": "^10.5.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "^1.4.0",
    "react-native-offline": "^6.0.0",
    "react-native-onesignal": "^4.5.1",
    "react-native-open-maps": "^0.4.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-permissions": "^3.6.1",
    "react-native-phone-input": "^1.3.5",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-signature-canvas": "^4.5.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "12.4.4",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.25.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "string-mask": "^0.3.0",
    "yup": "^0.32.11"
