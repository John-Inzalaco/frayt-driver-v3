  "devDependencies": {
    "@react-native-community/cli": "^10.0.0",
    "@redux-devtools/core": "^3.13.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.5.0",
    "@types/qs": "^6.9.7",
    "@types/react-native": "^0.70.8",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^29.5.0",
    "detox": "^20.1.3",
    "eslint": "7.32.0",
    "eslint-plugin-react-native": "4.0.0",
    "glob": "^8.0.3",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "prettier": "^2.7.1",
    "react-native-dotenv": "^3.4.6",
    "react-test-renderer": "18.1.0",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^14.14.0",
    "ts-jest": "^29.1.0",
    "typescript": "4.8.4"
  },
  "overrides": {
    "@typescript-eslint/typescript-estree": "5.41.0",
    "eslint": "7.32.0",
    "node-fetch": "3.2.10",
    "react-native-svg": "12.4.4"
  },
  "resolutions": {
    "@types/react": "^18.0.17"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__tests__/setup.ts",
      "<rootDir>/__tests__/__mocks__/blobMock.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "\\.svg$": "<rootDir>/__tests__/fileTransformer.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native(-.*)?|@react-navigation|@sentry|@frayt|@ui-kitten|@react-native(-community)?)/)"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
